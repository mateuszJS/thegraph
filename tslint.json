{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "interface-name": false,
    "ordered-imports": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "curly": false,
    "no-empty": false,
    "max-line-length": [true, 80],
    "semicolon": false,
    "quotemark": [true, "single", "jsx-double"],
    "jsx-no-multiline-js": false
  }
}